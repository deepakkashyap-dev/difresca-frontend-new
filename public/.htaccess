<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /
  RewriteRule ^index\.html$ - [L]
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
  RewriteRule ^ /index.html [L]
</IfModule>

# Fix MIME types
<IfModule mod_mime.c>
  AddType application/javascript js mjs
</IfModule>